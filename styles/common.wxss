/* 1. flex布局*/
  .dis-flex {
    display: flex !important;
  }
  .flex_wrap{
    flex-wrap: wrap;
  }
  .align-items {
    align-items: center;
  }
  
  .flex-box {
    flex: 1;
  }
  .flex-box2 {
    flex: 2;
  }

  
  .flex-dir-row {
    flex-direction: row;
  }
  
  .flex-dir-column {
    flex-direction: column;
  }
  
  .flex-x-center {
    justify-content: center;
  }
  
  .flex-x-between {
    justify-content: space-between;
  }
  
  .flex-x-around {
    justify-content: space-around;
  }
  
  .flex-x-end {
    justify-content: flex-end;
